{
  "expo": {
    "name": "Tibetskaya",
    "slug": "tibetskaya",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/512.png",
    "notification": {
      "icon": "./assets/images/48.png",
      "color": "#EF4130"  
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "buildNumber": "4",
      "bundleIdentifier": "com.tibetskaya.tibetskaya",
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"],
        "NSUserTrackingUsageDescription": "Для отправки уведомлений о статусе заказа.",
        "NSPushNotificationUsageDescription": "Мы используем уведомления для информирования вас о статусе вашего заказа."
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#EF4130",
      "translucent": false,
      "barStyle": "dark-content"
    },
    "android": {
      "newArchEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#EF4130"
      },
      "package": "com.tibetskaya.tibetskaya",
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/48.png",
          "color": "#EF4130",
          "defaultChannel": "default",
          "enableRemoteNotifications": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "7f565cac-ac07-46fe-819b-90eaf1474e90"
      }
    },
    "owner": "edil_kulzhabay"
  }
}
